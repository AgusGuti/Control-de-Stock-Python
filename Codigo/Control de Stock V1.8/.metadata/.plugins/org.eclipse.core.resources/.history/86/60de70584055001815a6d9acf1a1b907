from ClaseProducto import *
import Promo



#            (idProducto, nombre, stockCritico, cajas, cantidadPorCaja, precioXUnidad)

producto = Producto(1,"Cafe clasico chico",0,0,0,30)
dicCafe = {'clasico': producto}

producto= Producto(2,"Cafe especial chico",0,0,0,50)
dicCafe['especial'] = producto

#                            (IdProducto, nombre, cantidad vendida, precio)

promo = Promo(3,"5 chipa + Cafe clasico", 0, 60)
dicCafe['promo1'] = promo

promo = Promo(4,"5 chipa + Cafe especial", 0, 80)
dicCafe['promo2'] = promo

promo = Promo(5,"Cafe clasico + 2 Medialunas", 0,0)
dicCafe['promo1'] = promo

promo = Promo(6,"5 chipa + Cafe clasico", 0,0)
dicCafe['promo1'] = promo

