import time
import os
from _overlapped import NULL


# def crearArchivo():
nombre = time.asctime(time.localtime()) + ".txt"
nombre = nombre[0:10]+" - "+nombre[20:]

if (open(nombre,'r') == NULL):
    with open(nombre,'w') as fileStock:
        fileStock.write(" ")
        fileStock.close()
    pass
else:
    print("ERROR el archivo ya existe.")    
    pass
